{
  "name": "ao-frontend",
  "version": "1.0.5",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "cp src/index-app.js src/index.js && react-scripts start",
    "build": "cp src/index-app.js src/index.js && cross-env PUBLIC_URL='./' react-scripts build",
    "start:ico": "cp src/index-ico.js src/index.js && react-scripts start",
    "build:ico": "cp src/index-ico.js src/index.js && cross-env PUBLIC_URL='./' react-scripts build && npm run build:ico:inline",
    "build:ico:inline": "node scripts/inline-src.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
